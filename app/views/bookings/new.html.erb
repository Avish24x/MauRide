<h1>New Booking</h1>

<%= simple_form_for @booking do |form| %>
  <%= form.error_notification %>

  <div class="form-inputs">
    <%= form.input :seats %>
  </div>

  <div class="form-actions">
    <%= form.button :submit, "Create Booking" %>
  </div>
<% end %>

<%= link_to "Back", bookings_path %>
